@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index2</title>

</head>
<body>
    <div>
        <table border="1">
            <thead>
                <tr>
                    <td> ID</td>
                    <td>NAME</td>
                    <td>AGE</td>
                    <td>CITY</td>
                </tr>


            </thead>

            <tbody>
                <tr>
                    @*<td>@ViewBag.Id</td>
                        <td>@ViewBag.Name</td>
                        <td>@ViewBag.Age</td>
                        <td>@ViewBag.City</td>*@

                    @foreach (var item in Model)
                    {
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td>@item.Age</td>
                        <td>@item.City</td>
                    }
                </tr>
            </tbody>

        </table>

    </div>
    <p>
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>
